{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "import/no-named-as-default": 0,
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "class-methods-use-this": [
      "error",
      {
        "exceptMethods": [
          "validateUsername"
        ]
      }
    ]
  }
}